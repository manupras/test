#include <iostream>
using namespace std;
class A {
public:
	int a;
};

class B {
public:
	int b;
};

A operator+ (B b) {
	A a;
	a.a = a.a + b.b;
	return a;
}

int main() {
	cout << "cOpOver" << endl; // prints cOpOver
	return 0;
}
